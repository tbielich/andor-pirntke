---
pagination:
  data: cloudinary.folders
  size: 1
  alias: folder
  resolve: values
  tags: pages
permalink: "{{ folder.path | slugify }}/"
layout: base-folder
tags: pages
keywords:
  - Fahrzeug
---
<small><a href="/fahrzeuge/">Fahrzeuge</a></small>
<h1>{{ folder.name }}</h1>
{# <button type="button" name="button" value="2" id="button">2</button> #}
<section>
  {%- for image in folder.images -%}
  {%- if image.folder == folder.path %}
  {% set watermark = "https://res.cloudinary.com/andor-pirntke/image/upload/" + "co_rgb:00000075,l_text:Arial_48_bold:Andor-Pirntke_" + folder.name|slugify + ",g_south_east/v1625164956/" + image.public_id + "." + image.format %}
  {% thumb image.secure_url, watermark %}
  {%- endif -%}
  {%- endfor -%}
</section>
